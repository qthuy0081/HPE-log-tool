<Window x:Class="HPE_Log_Tool.Views.InitialPassword"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HPE_Log_Tool.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="400">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <PasswordBox x:Name="pwdPassword" FontSize="25" Height="30" Width="250" Margin="0 0 0 20"></PasswordBox>
        <Button FontSize="20" Content="Go" Click="Button_Click"></Button>
    </StackPanel>
</Window>
